<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	<!-- The Repast GUI displays these in alphabetical order -->

	<parameter name="randomSeed" displayName="Random Seed"
		type="int" defaultValue="32" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="maxTicks" displayName="Max Ticks" type="int"
		defaultValue="8000" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="gridHeight" displayName="Grid Height"
		type="int" defaultValue="80" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="gridWidth" displayName="Grid Width"
		type="int" defaultValue="80" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="numBees"
		displayName="Initial Bee Population" type="int" defaultValue="10"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="foodPerFlower"
		displayName="Food Per Flower" type="int" defaultValue="3"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="flowerDensity"
		displayName="Initial Flower Density" type="double"
		defaultValue="0.128" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="sightRadius" displayName="Bee Sight Radius"
		type="int" defaultValue="2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="beeRegenRate"
		displayName="Bee Regeneration Rate" type="double" defaultValue="0.01"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="beeCost" displayName="Cost of New Bee"
		type="int" defaultValue="2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="deathProb"
		displayName="Bee Death Probability" type="double"
		defaultValue="0.0001" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="flowerRegenRate"
		displayName="Flower Regeneration Rate" type="double"
		defaultValue="0.01" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="herbicideDriftProb"
		displayName="Herbicide Drift Probability" type="double"
		defaultValue="1.0E-4" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

	<parameter name="bearAttackProb"
		displayName="Bear Attack Probability" type="double"
		defaultValue="0.001" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />

</parameters>
